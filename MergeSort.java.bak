public class MergeSort {
    public static void main(String[] args) {
        int[] arr = {5, 2, 9, 1, 3};
        Divide(arr, 0, arr.length - 1);
        for (int num : arr) {
            System.out.print(num + " ");
        }
    }
   static void Divide(int[] arr, int left, int right) {
