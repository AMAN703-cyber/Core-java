import java.util.Scanner;
class Swapping
{
	public static void main(String[] args) 
	{
		int a,b;
		Scanner scr = new Scanner(System.in);
		System.out.println("Enter the value of a before swapping :");
		a=scr.nextInt();
		System.out.println("Enter the value of b before swapping :");
		b=scr.nextInt();
		a=a+b;
		b=a-b;
		a=a-b;
		System.out.println("Enter the value of a after swapping :");
		a=scr.nextInt();
		System.out.println("Enter the value of b after swapping :");
		b=scr.nextInt();
	}
}
