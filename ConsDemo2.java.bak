class ConsDemo2 
{
	int a,b;
	ConsDemo2()
	{
		this(1000);
	}
	ConsDemo2(int x)
	{
		this(x,2000);
	}
	ConsDemo2(int x, int y)
	{
		a=x;
		b=y;
	}
	void m2()
	{
		System.out.print("Ans="=+(a+b));
	}
	public static void main(String[] args) 
	{
		ConsDemo2 d1 = new ConsDemo2();
		d1.m2();
	}
}
